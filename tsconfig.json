{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "lib": [
      "ES2020"
    ],
    "outDir": "out",
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "ts/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
}
